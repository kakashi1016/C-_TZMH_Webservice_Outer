@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




    <form class="" id="submitPayment" action="/alipay/payment" method="post">
        @Html.AntiForgeryToken()
            <legend class="">预缴款充值</legend>
       

        <div class="container-fluid">



            <div class="row">
                <div class="col-xs-12 col-md-12 col-lg-12 col-sm-12">

                    <div class="form-group">
                        <label >充值流水号</label>
                        <input type="text" class="form-control" id="flowid" name="flowid" placeholder="请输入充值流水号">
                    </div>
                    <div class="form-group">
                        <label for="desc">充值信息</label>
                        <input type="text" class="form-control" id="desc" name="desc" placeholder="请输入充值信息">
                    </div>
                    <div class="form-group">
                        <label for="fee">充值金额</label>
                        <input type="text" class="form-control" id="fee" name="fee" placeholder="请输入充值金额">
                    </div>


                </div>
            </div>

            <div class="row">
                <div class="col-xs-12 col-md-12 col-lg-12">
                    <button class="btn btn-block btn-primary btn-large" type="button" onclick="submit();" id="confirm">确认冲缴</button>
                </div>
            </div>
        </div>

    </form>




